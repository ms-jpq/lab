location /m5_assert([ENV_PATH]) {
  return 307 /m5_assert([ENV_PATH])/;
}

location /m5_assert([ENV_PATH])/ {
  error_log                 stderr crit;
  proxy_http_version        1.0;
  chunked_transfer_encoding off;
  proxy_buffering           off;
  proxy_pass                http://unix:m5_assert([ENV_SOCK]):/;
}
