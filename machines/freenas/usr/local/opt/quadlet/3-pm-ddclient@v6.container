[Unit]
Description   = Podman -- %J@%I

After         = network-online.target

[Container]
Image         = lscr.io/linuxserver/ddclient:latest
ContainerName = %J-%I
AutoUpdate    = registry

# Network     = host
Volume        = /usr/local/opt/%J/%I:/config

[Service]
Restart       = on-failure

[Install]
WantedBy      = default.target
