[Unit]
Description  = Mount -- %J:%f

[Mount]
Type         = rclone
ForceUnmount = yes
TimeoutSec   = 9seconds
Options      = _netdev,args2env,allow-non-empty,allow-other,uid=1000,gid=1000,config=/dev/null,webdav-vendor=rclone,webdav-pacer-min-sleep=0,vfs-cache-mode=full,cache-dir=%C/local/rclone/%p,dir-cache-time=9s,poll-interval=8s,http-url=http://freenas.enp2s0.pfsense.home.arpa:8080/webdav/rw/,ro
Where        = %f
What         = :http:
