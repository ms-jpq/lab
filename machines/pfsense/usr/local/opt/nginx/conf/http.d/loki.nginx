server {
  server_name loki.*;
  listen      unix:/run/local/nginx/gateway-https.sock;

  location / {
    auth_basic           "-";
    auth_basic_user_file /var/lib/local/nginx/loki.htpasswd;
    proxy_set_header     Host freenas.eno1.pfsense.localhost;
    proxy_pass           http://unix:/run/local/nginx/gateway-https.sock:;
  }
}